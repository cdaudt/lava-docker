{% extends 'base.jinja2' %}
{% block body %}
device_type: 943907AEVAL1F
board_id: '{{ board_id|default('0000000000') }}'

actions:
  boot:
    connections:
      serial:
      ssh:
    methods:
      pyocd:
        parameters:
          command:
            /opt/cyp_ocd/wiced_ocd.py
          options:
          - -d {{ debug|default('debug') }}
          - -t {{ target|default('k64f') }}
          - -f {{ frequency|default('3000000') }}
{% endblock -%}
